<td><ul class="nav nav-pills"><li class="active" ><%= link_to '+', new_saida_path %></li></ul></td>


<h1>Listing saidas</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Mp</th>
      <th>Saída</th>
      <th>Qtde</th>
      <th>Nf de saída</th>
      <th>Lote</th>
      <th>Validade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saidas.each do |saida| %>
      <tr>
        <td><%= saida.mp.nome %></td>
        <td><%= saida.exped %></td>
        <td><%= saida.qtde %></td>
        <td><%= saida.nf %></td>
        <td><%= saida.lote %></td>
        <td><%= saida.validade %></td>
        <td>
          <ul class="nav nav-pills">
            <li class="active">
              <td><%= link_to 'Show', saida %></td>
            </li>
          </ul>
        </td>

                <td>
          <ul class="nav nav-pills">
            <li class="active">
              <td><%= link_to 'Edit', edit_saida_path(saida) %></td>
            </li>
          </ul>
        </td>

                <td>
          <ul class="nav nav-pills">
            <li class="active">
              <%= link_to 'Destroy', saida, method: :delete, data: { confirm: 'Are you sure?' } %>
            </li>
          </ul>
        </td>
        
        
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>


